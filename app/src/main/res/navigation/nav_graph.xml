<?xml version="1.0" encoding="utf-8"?>

<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/MainActivity">

    <fragment
        android:id="@+id/MainActivity"
        android:name="com.example.esperanto_gruppeh1.MainActivity"
        android:label="@string/app_name">
        <action
            android:id="@+id/action_mainactivity_to_playerfragment"
            app:destination="@id/SearchFragment" />
    </fragment>
    <fragment
        android:id="@+id/SearchFragment"
        android:name="com.example.esperanto_gruppeh1.SearchFragment"
        android:label="@string/search_fragment_label"
        tools:layout="@layout/fragment_search_list">
        <argument
            android:name="letter"
            app:argType="string" />
    </fragment>
</navigation>